{
  "expo": {
    "name": "柚子",
    "description": "Example app for React Native Paper: https://callstack.github.io/react-native-paper/",
    "slug": "bd-chat-app",
    "privacy": "public",
    "version": "2.0.0",
    "orientation": "default",
    "primaryColor": "#2096d4",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/paper-icon.png",
    "packagerOpts": {
      "config": "./metro.config.js",
      "projectRoots": ""
    },
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "owner": "react-native-paper",
    "ios": {
      "bundleIdentifier": "com.callstack.reactnativepaperexample",
      "buildNumber": "4.0.0"
    },
    "android": {
      "package": "com.callstack.reactnativepaperexample",
      "versionCode": 6,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "icon": "./assets/images/paper-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundColor": "#6200EE"
      }
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#2096d4",
      "resizeMode": "cover"
    },
    "plugins": [
      [
        "expo-community-flipper",
        {
          "ios": {
            "enabled": true,
            "stripUseFrameworks": true
          },
          "android": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
